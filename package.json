{
  "name": "delightree-analytics-api",
  "version": "1.0.0",
  "description": "GraphQL API for sales analytics with MongoDB",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/functions/graphql.js",
    "lint": "eslint .",
    "test": "jest",
    "build": "webpack --config webpack.config.js",
    "vercel-build": "webpack --config webpack.config.js",
    "netlify-build": "netlify-lambda build src/functions",
    "start:lambda": "netlify-lambda serve functions -c webpack.config.js",
    "seed": "node src/seeds/seed.js"
  },
  "dependencies": {
    "apollo-server-express": "^3.12.1",
    "apollo-server-lambda": "^3.12.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.5.0",
    "mongoose": "6.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^9.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "netlify-lambda": "^2.0.15",
    "nodemon": "^2.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "graphql",
    "mongodb",
    "apollo",
    "express",
    "sales-analytics"
  ],
  "author": "Akshat Sachan",
  "license": "MIT"
}
